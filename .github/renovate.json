{
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "packagePatterns": ["^eslint"],
      "groupName": "eslint"
    },
    {
      "packagePatterns": ["gatsby"],
      "groupName": "gatsby"
    },
    {
      "packageNames": ["react", "react-dom"],
      "groupName": "react"
    }
  ],
  "labels": ["renovate"],
  "prHourlyLimit": 4,
  "prConcurrentLimit": 16,
  "bumpVersion": null,
  "semanticCommits": true,
  "automerge": true,
  "schedule": ["after 2am and before 5am every weekday"]
}
